%   Detailed explanation goes here

tol=10^(-4);
flag=1;

[m,~] = size(A);
v = rand(m);

L = length(lambda);

for i = 1 : L,
mu = lambda(i)
while flag > tol,
    
   ;
   b = A-mu*eye(m);
   [Q, R] = qr(b);
   
   w = backSub(R, Q'*v)';
   
   vnew = w/norm(w);
    
   munew=vnew'*A*vnew;
   flag=norm(lambdanew-lambda);
   lambda= lambdanew;
   v=vnew;


end

V(:, i) = v;

end


end

